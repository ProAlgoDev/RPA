{
  "manifest_version": 2,
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "short_name": "__MSG_short_name__",
  "default_locale": "en",
  "version": "3.1.3",
  "icons": {
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": {
      "38": "logo.png"
    }
  },
  "background": {
    "scripts": [
      "vendor.js",
      "background_csv_editor_popup_vision_editor.js",
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "content_script.js",
    "inject.js",
    "logo.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "bookmarks",
    "clipboardRead",
    "clipboardWrite",
    "cookies",
    "downloads",
    "notifications",
    "storage",
    "tabs",
    "<all_urls>"
  ]
}